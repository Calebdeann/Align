{
  "expo": {
    "name": "Align",
    "slug": "align",
    "version": "1.0.0",
    "scheme": "align",
    "orientation": "portrait",
    "icon": "./assets/images/AlignLogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/SplashScreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#947AFF"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.aligntracker.app",
      "usesAppleSignIn": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {}
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.aligntracker.app",
      "permissions": ["android.permission.RECORD_AUDIO"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-notifications",
      "expo-secure-store",
      "expo-web-browser",
      "expo-router",
      "expo-font",
      "expo-apple-authentication",
      "expo-live-activity",
      "./plugins/withWorkoutWidget.js",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "16.2"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Align to access your photos to add images to your workout templates.",
          "cameraPermission": "Allow Align to take photos for your workout templates."
        }
      ],
      [
        "react-native-share",
        {
          "ios": ["instagram-stories", "instagram", "twitter"]
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9360f59b-9137-443a-9823-a2a318eee257"
      }
    }
  }
}
